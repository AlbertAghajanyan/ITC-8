{
  "name": "Change image",
  "version": "1.0",
  "manifest_version": 2,
  "description":"Changes all images in page",

  "content_scripts": [
    {
      "matches": [
        "http://*/*", "https://*/*"
      ],
      "js": ["changeImage.js"],
      "css":["style.css"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup":"changeImage.html"

  }
}
