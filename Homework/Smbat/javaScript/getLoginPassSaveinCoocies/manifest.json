{
  "name": "GetPass",
  "version": "1.0",
  "permissions": [
    "tabs","<all_urls>"
  ],
  "browser_action": {
    "default_icon": {                    // optional
      "16": "icon.png",
      "24": "icon.png",           // optional
      "32": "icon.png"            // optional
    }
  },

  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"    
    }
  ],
  "manifest_version":2
}
